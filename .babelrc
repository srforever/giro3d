{
    "presets": ["env"],
    "plugins": [
        ["babel-plugin-inline-import", {
            "extensions": [
                ".json",
                ".glsl"
            ]
        }],
        "transform-runtime",
        ["minify-replace", {
            "replacements": [{
                "identifierName": "__DEBUG__",
                "replacement": {
                    "type": "booleanLiteral",
                    "value": false
                }
            }]
        }],
        ["minify-dead-code-elimination"]
    ]
}
