{
  "name": "@giro3d/giro3d",
  "version": "0.3.0",
  "description": "A JS/WebGL framework for 3D geospatial data visualization",
  "scripts": {
    "commit": "npx git-cz",
    "lint-commit": "commitlint -x @commitlint/config-conventional --from=origin/HEAD --to=HEAD",
    "lint": "eslint \"src/**/*.js\" \"utils/**/*.js\" \"test/**/*.js\" \"examples/**/*.js\"",
    "apidoc": "shx rm -rf build/site/apidoc && jsdoc -R jsdoc/api/index.html -c jsdoc/api/conf.json -P package.json -d build/site/apidoc",
    "build-site": "mkdir -p build/site && cp -v site/index.html build/site/ && cp -v site/favicon* build/site/ && npm run build-examples && npm run apidoc",
    "build-package": "npm run transpile && node tasks/prepare-package.mjs",
    "doclint": "npm run apidoc -- -t templates/silent",
    "test": "npm run lint -- --max-warnings=0 && npm run build && npm run test-examples && npm run test-with-coverage",
    "test-unit": "jest test/unit",
    "test-examples": "mocha -t 300000 --recursive test/examples",
    "test-with-coverage": "jest test/unit --coverage",
    "transpile": "shx rm -rf build/giro3d && shx mkdir -p build/giro3d && shx cp -rf src build/giro3d/src && cross-env BABEL_DISABLE_CACHE=1 babel src --source-maps --out-dir build/giro3d",
    "serve-examples": "webpack serve --config examples/webpack/config.mjs --mode=development",
    "build-examples": "webpack --config examples/webpack/config.mjs --mode=production",
    "generate-examples": "node tasks/generate-examples.mjs",
    "prepack": "npm run build && npm run transpile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/giro3d/giro3d.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/giro3d/giro3d/-/issues"
  },
  "homepage": "https://giro3d.org/",
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@mapbox/togeojson": "^0.16.0",
    "@mapbox/vector-tile": "^1.3.0",
    "earcut": "^2.1.1",
    "flatbush": "^3.1.0",
    "geotiff": "^2.0.5",
    "js-priority-queue": "^0.1.5",
    "pbf": "^3.1.0",
    "text-encoding-utf-8": "^1.0.1"
  },
  "peerDependencies": {
    "ol": "^6.14.1",
    "proj4": "^2.8.0",
    "three": "^0.135.0",
    "three.meshline": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/register": "^7.17.7",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jest": "^28.1.3",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "babel-plugin-minify-replace": "^0.5.0",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^3.1.4",
    "dat.gui": "^0.7.9",
    "es-main": "^1.2.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-webpack-plugin": "^2.4.1",
    "front-matter": "^4.0.2",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "jsdoc": "3.6.3",
    "jsdoc-plugin-typescript": "^2.0.5",
    "jsdom": "^20.0.0",
    "marked": "^0.3.9",
    "node-notifier": "^10.0.1",
    "nyc": "^12.0.2",
    "ol": "^6.14.1",
    "proj4": "^2.8.0",
    "puppeteer": "^1.5.0",
    "query-selector": "^2.0.0",
    "shx": "^0.3.2",
    "three": "^0.135.0",
    "three.meshline": "^1.0.3",
    "url-polyfill": "^1.1.12",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "whatwg-fetch": "^3.6.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!ol|three|quick-lru)"
    ],
    "testEnvironment": "jsdom"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
