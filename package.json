{
  "name": "@giro3d/giro3d",
  "version": "0.0.2-osl",
  "description": "A JS/WebGL framework for 3D geospatial data visualization",
  "main": "lib/Main.js",
  "scripts": {
    "lint": "eslint \"src/**/*.js\" \"utils/**/*.js\" \"test/**/*.js\" \"examples/**/*.js\"",
    "apidoc": "shx rm -rf build/apidoc && jsdoc -R jsdoc/api/index.md -c jsdoc/api/conf.json -P package.json -d build/apidoc",
    "build-site": "mkdir -p build && cp -v site/index.html build/ && cp -rv examples build/  && cp -v index.html examples/ && npm run apidoc",
    "doclint": "npm run apidoc -- -t templates/silent",
    "test": "npm run lint -- --max-warnings=0 && npm run build && npm run test-examples && npm run test-with-coverage",
    "test-unit": "jest test/unit",
    "test-examples": "mocha -t 300000 --recursive test/examples",
    "test-with-coverage": "nyc -n src -r html cross-env npm run test-unit",
    "build": "webpack -p",
    "transpile": "cross-env BABEL_DISABLE_CACHE=1 babel src --source-maps --out-dir lib",
    "start": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 webpack-dev-server -d --inline --hot",
    "prepack": "npm run build && npm run transpile"
  },
  "files": [
    "*.md",
    "dist",
    "lib",
    "doc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oslandia/giro3d.git"
  },
  "license": "(CECILL-B OR MIT)",
  "bugs": {
    "url": "https://github.com/Oslandia/giro3d/issues"
  },
  "homepage": "https://giro3d.github.io/",
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@mapbox/togeojson": "^0.16.0",
    "@mapbox/vector-tile": "^1.3.1",
    "babel-polyfill": "^6.26.0",
    "earcut": "^2.2.3",
    "flatbush": "^4.0.0",
    "js-priority-queue": "^0.1.5",
    "pbf": "^3.2.1",
    "text-encoding-utf-8": "^1.0.2",
    "url-polyfill": "^1.1.12",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "proj4": "^2.8.0",
    "three": "^0.124.0",
    "three.meshline": "^1.4.0",
    "ol": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.4",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-minify-replace": "^0.5.0",
    "chart.js": "^2.4.0",
    "cross-env": "^3.1.4",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-webpack-plugin": "^2.4.1",
    "jest": "^26.4.2",
    "jsdoc": "3.6.3",
    "jsdoc-plugin-typescript": "^2.0.5",
    "marked": "^0.3.9",
    "nyc": "^12.0.2",
    "ol": "^5.2.0",
    "proj4": "^2.8.0",
    "puppeteer": "^1.5.0",
    "shx": "^0.3.2",
    "three": "^0.124.0",
    "three.meshline": "^1.4.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!ol|three)"
    ]
  }
}
