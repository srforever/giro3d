{
  "opts": {
    "recurse": true,
    "template": "jsdoc/api/template",
    "sourcePath": "src/"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "src/core/Context.js",
      "src/core/geographic/Coordinates.js",
      "src/core/geographic/Extent.js",
      "src/core/Instance.js",
      "src/core/layer/ColorLayer.js",
      "src/core/layer/ColorMap.js",
      "src/core/layer/ColorMapMode.js",
      "src/core/layer/ElevationLayer.js",
      "src/core/layer/Interpretation.js",
      "src/core/layer/Layer.js",
      "src/core/layer/LayerUpdateStrategy.js",
      "src/core/Points.js",
      "src/entities",
      "src/formats",
      "src/gui",
      "src/helpers",
      "src/interactions",
      "src/renderer/Camera.js",
      "src/renderer/PointsMaterial.js",
      "src/sources"
    ]
  },
  "plugins": [
    "jsdoc/api/plugins/markdown",
    "jsdoc-plugin-typescript",
    "jsdoc/api/plugins/inline-options",
    "jsdoc/api/plugins/events",
    "jsdoc/api/plugins/observable",
    "jsdoc/api/plugins/api"
  ],
  "typescript": {
    "moduleRoot": "src"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    },
    "applicationName": "Giro3D"
  },
  "jsVersion": 180
}
